{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=TestApiDb;Username=postgres_user;Password=postgres_password"
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 10240,
      "MaxRequestHeaderCount": 20
    },
    "EndpointDefaults": {
      "Protocols": "Http1"
    },
    "Endpoints": {
      "Grpc": {
        "Protocols": "Http2",
        "Url": "http://0.0.0.0:5006"
      },
      "WebApi": {
        "Protocols": "Http1",
        "Url": "http://0.0.0.0:5005"
      }
    }
  }
}